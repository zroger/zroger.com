---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>zroger.com | {{ page.category | capitalize }}</title>
  <link href="http://zroger.com{% category_url page.category %}"/>
  <link type="application/atom+xml" rel="self" href="http://zroger.com{% category_url page.category %}/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://zroger.com{% category_url page.category %}</id>
  <author>
    <name>Roger López</name>
  </author>

  {% for post in site.categories[page.category] %}
  <entry>
    <id>http://zroger.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://zroger.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Roger López</name>
      <uri>http://zroger.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
